CREATE TABLE [dbo].[QuestionTypes] (
    [questionTypeId] INT          NOT NULL,
    [questionType]   VARCHAR (20) NULL,
    PRIMARY KEY CLUSTERED ([questionTypeId] ASC)
);




INSERT INTO [dbo].[QuestionTypes] ([questionTypeId], [questionType]) VALUES (1, N'RADIO')
