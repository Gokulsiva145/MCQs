CREATE TABLE [dbo].[Results] (
    [Id]            INT          IDENTITY (1, 1) NOT NULL,
    [exam_Id]       INT          NULL,
    [student_RegNo] VARCHAR (50) NULL,
    [question_No]   INT          NULL,
    [writtenAnswer] VARCHAR (50) NULL,
    CONSTRAINT [PK_Results] PRIMARY KEY CLUSTERED ([Id] ASC)
);



CREATE TABLE [dbo].[Schedules] (
    [examId]       INT           NOT NULL,
    [examCode]     VARCHAR (50)  NOT NULL,
    [subject_Code] VARCHAR (100) NULL,
    [examDate]     DATE          NULL,
    [startsFrom]   TIME (7)      NULL,
    [endTo]        TIME (7)      NULL,
    [passMark]     FLOAT (53)    NULL,
    [examType]     INT           NULL,
    [createdBy]    VARCHAR (50)  NULL,
    PRIMARY KEY CLUSTERED ([examId] ASC),
    UNIQUE NONCLUSTERED ([examCode] ASC)
);

