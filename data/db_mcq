CREATE TABLE [dbo].[ExamFor] (
    [serialNo]      INT          IDENTITY (1, 1) NOT NULL,
    [exam_Id]       INT          NULL,
    [studentRegNo]  VARCHAR (50) NULL,
    [writtenStatus] INT          NULL,
    CONSTRAINT [PK_ExamFor] PRIMARY KEY CLUSTERED ([serialNo] ASC)
);




CREATE TABLE [dbo].[ExamQuestions] (
    [Id]               INT            NOT NULL,
    [exam_Id]          INT            NULL,
    [questionNo]       INT            NULL,
    [question]         NVARCHAR (MAX) NULL,
    [optionA]          NVARCHAR (50)  NULL,
    [optionB]          NVARCHAR (50)  NULL,
    [optionC]          NVARCHAR (50)  NULL,
    [optionD]          NVARCHAR (50)  NULL,
    [correctAnswer]    NVARCHAR (50)  NULL,
    [mark]             FLOAT (53)     NULL,
    [negativeMark]     FLOAT (53)     NULL,
    [question_Type_Id] INT            NULL,
    PRIMARY KEY CLUSTERED ([Id] ASC)
);


INSERT INTO [dbo].[ExamQuestions] ([Id], [exam_Id], [questionNo], [question], [optionA], [optionB], [optionC], [optionD], [correctAnswer], [mark], [negativeMark], [question_Type_Id]) VALUES (1, 1, 1, N'who is the richest person in the world?', N'jeff', N'arnold', N'elan', N'mukesh', N'elan', 2, 1.5, 1)
